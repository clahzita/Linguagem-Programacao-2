
public class main {

	public static void main(String[] args) {
		AreaCalculavel circulo = new Circulo(5.0);
		AreaCalculavel quadrado = new Quadrado(12.0);
		
		System.out.println("Area do"+ circulo.getClass().toString().replaceAll("class","") +": "+circulo.calcularArea());
		System.out.println("Area do"+ quadrado.getClass().toString().replaceAll("class","") +": "+quadrado.calcularArea());

	}

}
