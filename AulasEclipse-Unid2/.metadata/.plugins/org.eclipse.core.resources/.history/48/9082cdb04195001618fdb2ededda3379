 * 
 */
package br.imd.clarissa.entidades;

/**
 * @author clarissa
 *
 */
public class Porta implements Cloneable {
	private double altura;
	private double largura;
	private boolean aberta;
	public Porta(double altura, double largura) {
		super();
		this.altura = altura;
		this.largura = largura;
		this.aberta = false;
	}
	
	

}
