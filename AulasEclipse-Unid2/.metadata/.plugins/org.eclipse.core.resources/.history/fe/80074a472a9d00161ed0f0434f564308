package br.imd.teste;

import static org.junit.Assert.*;



import org.junit.Test;

import br.imd.entidade.Pessoa;

public class PessoaTest {
	Pessoa pessoa1 = new Pessoa("Laura", "20/11/1988", "06570555499", "3333-9090");
	Pessoa pessoa2= new Pessoa("Fernanda", "12/03/1980", "06570555499", "3333-9090");
	@Test
	public void idadetest() {
		assertEquals(27,pessoa1.getIdade(),0.0001);
		assertEquals(36,pessoa1.getIdade(),0.0001);
		
	}

}
