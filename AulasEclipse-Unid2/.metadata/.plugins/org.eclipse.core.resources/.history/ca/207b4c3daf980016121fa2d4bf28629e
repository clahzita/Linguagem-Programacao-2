package br.com.locavel.dao;

import java.util.ArrayList;

import br.com.locavel.banco.Banco;
import br.com.locavel.entidades.Veiculo;

/**
 * @author clarissa
 * @version 1.0
 */
public class CarroDao implements GenericDao {

	/* (non-Javadoc)
	 * @see br.com.locavel.dao.GenericDao#inserir(br.com.locavel.entidades.Veiculo)
	 */
	@Override
	public void inserir(Veiculo e) {
		Banco.veiculosCadastrados.add(e);

	}

	/* (non-Javadoc)
	 * @see br.com.locavel.dao.GenericDao#excluir(br.com.locavel.entidades.Veiculo)
	 */
	@Override
	public void excluir(Veiculo e) {
		Banco.veiculosCadastrados.remove(e);

	}

	/* (non-Javadoc)
	 * @see br.com.locavel.dao.GenericDao#listarTodos()
	 */
	@Override
	public ArrayList<Veiculo> listarTodos() {
		
		return null;
	}

}
