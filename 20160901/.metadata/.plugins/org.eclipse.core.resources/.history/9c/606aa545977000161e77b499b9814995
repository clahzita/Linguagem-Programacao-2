
public class Retangulo extends Figura {
	private double altura;
	private double base;

	public Retangulo(double altura,double base) {
		this.base = base;
		this.altura = altura;
	}
	
//	public double calcularArea(){
//		return lado*lado;
//		
//	}
	
	@Override
	public double calcularArea() {
		return this.base*this.base;
	}
	

}
